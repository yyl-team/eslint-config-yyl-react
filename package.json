{
  "name": "eslint-config-yyl-react",
  "version": "3.0.1",
  "description": "eslint config for yyl-react",
  "main": "./eslint.config.js",
  "type": "module",
  "scripts": {
    "u": "yarn upgrade-interactive --latest",
    "eslint": "eslint --config ./eslint.config.js ./test/base/src",
    "prettier": "prettier --write ./test/base/src/**/*",
    "test": "yarn eslint && yarn prettier && yarn eslint",
    "prepublishOnly": "yarn test"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/eslint-config-yyl-react.git"
  },
  "keywords": [
    "eslint-config",
    "yyl-react"
  ],
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/eslint-config-yyl-react/issues"
  },
  "homepage": "https://github.com/jackness1208/eslint-config-yyl-react#readme",
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "prettier": ">=3.0.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.18.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.9",
    "@types/react": "^19.0.7",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "typescript": "^5.7.3"
  }
}
