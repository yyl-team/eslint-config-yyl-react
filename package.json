{
  "name": "eslint-config-yyl-react",
  "version": "2.1.0",
  "description": "eslint config for yyl-react",
  "main": "index.js",
  "scripts": {
    "u": "yarn upgrade-interactive --latest",
    "eslint": "eslint --config ./.eslintrc.json --ext=jsx,ts,tsx ./test/base/src",
    "prettier": "prettier --write ./test/base/src/**/*",
    "test": "yarn eslint && yarn prettier && yarn eslint",
    "prepublishOnly": "yarn test"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/eslint-config-yyl-react.git"
  },
  "keywords": [
    "eslint-config",
    "yyl-react"
  ],
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/eslint-config-yyl-react/issues"
  },
  "homepage": "https://github.com/jackness1208/eslint-config-yyl-react#readme",
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "prettier": ">=2.0.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.8",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "typescript": "^5.1.3"
  }
}
